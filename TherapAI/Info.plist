<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <!-- App Information -->
    <key>CFBundleDisplayName</key>
    <string>TherapAI</string>
    <key>CFBundleName</key>
    <string>TherapAI</string>
    <key>CFBundleShortVersionString</key>
    <string>1.0</string>
    <key>CFBundleVersion</key>
    <string>1</string>
    
    <!-- Privacy Policy URL -->
    <key>NSPrivacyPolicyURL</key>
    <string>https://therapai.app/privacy</string>
    
    <!-- Usage Descriptions for Privacy -->
    <key>NSCameraUsageDescription</key>
    <string>TherapAI does not access your camera. This permission is not used.</string>
    
    <key>NSMicrophoneUsageDescription</key>
    <string>TherapAI does not access your microphone. This permission is not used.</string>
    
    <key>NSLocationWhenInUseUsageDescription</key>
    <string>TherapAI does not access your location. This permission is not used.</string>
    
    <key>NSLocationAlwaysAndWhenInUseUsageDescription</key>
    <string>TherapAI does not access your location. This permission is not used.</string>
    
    <key>NSContactsUsageDescription</key>
    <string>TherapAI does not access your contacts. This permission is not used.</string>
    
    <key>NSCalendarsUsageDescription</key>
    <string>TherapAI does not access your calendar. This permission is not used.</string>
    
    <key>NSPhotoLibraryUsageDescription</key>
    <string>TherapAI does not access your photo library. This permission is not used.</string>
    
    <key>NSHealthShareUsageDescription</key>
    <string>TherapAI does not access your health data. This permission is not used.</string>
    
    <key>NSHealthUpdateUsageDescription</key>
    <string>TherapAI does not update your health data. This permission is not used.</string>
    
    <!-- Network Usage -->
    <key>NSAppTransportSecurity</key>
    <dict>
        <key>NSAllowsArbitraryLoads</key>
        <false/>
        <key>NSExceptionDomains</key>
        <dict>
            <key>openrouter.ai</key>
            <dict>
                <key>NSExceptionAllowsInsecureHTTPLoads</key>
                <false/>
                <key>NSExceptionMinimumTLSVersion</key>
                <string>TLSv1.2</string>
                <key>NSExceptionRequiresForwardSecrecy</key>
                <true/>
            </dict>
            <key>firebase.googleapis.com</key>
            <dict>
                <key>NSExceptionAllowsInsecureHTTPLoads</key>
                <false/>
                <key>NSExceptionMinimumTLSVersion</key>
                <string>TLSv1.2</string>
                <key>NSExceptionRequiresForwardSecrecy</key>
                <true/>
            </dict>
        </dict>
    </dict>
    
    <!-- Support for Crisis Resources -->
    <key>LSApplicationQueriesSchemes</key>
    <array>
        <string>tel</string>
        <string>sms</string>
    </array>
    
    <!-- App Store Age Rating -->
    <key>ITSAppUsesNonExemptEncryption</key>
    <false/>
    
    <!-- Support Contact -->
    <key>NSHumanReadableCopyright</key>
    <string>Copyright Â© 2024 TherapAI. All rights reserved.</string>
    
    <!-- Background Modes (if needed) -->
    <key>UIBackgroundModes</key>
    <array>
        <string>background-processing</string>
    </array>
    
    <!-- Supported Interface Orientations -->
    <key>UISupportedInterfaceOrientations</key>
    <array>
        <string>UIInterfaceOrientationPortrait</string>
        <string>UIInterfaceOrientationLandscapeLeft</string>
        <string>UIInterfaceOrientationLandscapeRight</string>
    </array>
    
    <key>UISupportedInterfaceOrientations~ipad</key>
    <array>
        <string>UIInterfaceOrientationPortrait</string>
        <string>UIInterfaceOrientationPortraitUpsideDown</string>
        <string>UIInterfaceOrientationLandscapeLeft</string>
        <string>UIInterfaceOrientationLandscapeRight</string>
    </array>
</dict>
</plist>